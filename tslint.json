{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "prefer-const": false,
    "await-promise": true,
    "no-any": false,
    "quotemark": false,
    "max-classes-per-file": false,
    "no-empty": false,
    "no-string-throw": false,
    "no-shadowed-variable": false
  },
  "rulesDirectory": []
}
